<CFSDatabaseReader>
  <processes>
    <!-- Initialization Data -->
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportLookupTables</module>
    <!-- Users, Accounts, Contacts and ownership -->
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportBaseData</module>
    <!-- Related Data -->
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportOpportunities</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportOpportunityReports</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportAccountReports</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportRevenue</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportTickets</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportFolders</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportProjects</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportDocuments</module>
    <module>com.darkhorseventures.apps.dataimport.cfsdatabasereader.ImportCommunications</module>
  </processes>
  <mappings>
    <map class="com.darkhorseventures.cfsbase.Contact" id="contact">
      <property>guid</property>
      <property lookup="account">orgId</property>
      <property>title</property>
      <property>department</property>
      <property>nameSalutation</property>
      <property>nameFirst</property>
      <property>nameMiddle</property>
      <property>nameLast</property>
      <property>nameSuffix</property>
      <property lookup="">typeId</property>
      <property>site</property>
      <property>notes</property>
      <property>locale</property>
      <property>employeeId</property>
      <property lookup="">employmentType</property>
      <property lookup="user">enteredBy</property>
      <property>enabled</property>
      <property lookup="user">owner</property>
      <property>custom1</property>
      <property>url</property>
      <property lookup="user">userId</property>
    </map>
    <map class="com.darkhorseventures.cfsbase.Organization" id="account">
      <property>guid</property>
      <property>name</property>
      <property>url</property>
      <property>notes</property>
      <property lookup="">industry</property>
      <property>miner_only</property>
      <property lookup="user">enteredBy</property>
      <property>contactEndDate</property>
      <property>alertDate</property>
      <property>alertText</property>
      <property>enabled</property>
      <property>employees</property>
      <property>revenue</property>
      <property>ticker</property>
      <property>accountNumber</property>
      <property lookup="user">owner</property>
    </map>
    <map class="com.darkhorseventures.cfsbase.User" id="user">
      <property alias="guid">id</property>
      <property>username</property>
      <property>encryptedPassword</property>
      <property lookup="contact">contactId</property>
      <property lookup="role">roleId</property>
      <property lookup="user">managerId</property>
      <property>timeZone</property>
      <property lookup="user">enteredBy</property>
      <property>expires</property>
      <property>aliasName</property>
      <property lookup="user">assistant</property>
      <property lookup="user">alias</property>
    </map>
    <map class="com.darkhorseventures.cfsbase.Role" id="role">
      <property>guid</property>
      <property>role</property>
      <property>description</property>
      <property>enteredBy</property>
      <property>modifiedBy</property>
      <property>enabled</property>
    </map>
  </mappings>
</CFSDatabaseReader>
