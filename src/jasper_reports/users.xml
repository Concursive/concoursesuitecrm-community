	<queryString><![CDATA[SELECT myuser.user_id, myuser.username, 
myuser.last_login, myuser.expires, myuser.enabled, 
c.namefirst, c.namelast, c.title,
r.role, 
manager.namefirst AS manager_namefirst, 
manager.namelast AS manager_namelast  
FROM access myuser
LEFT JOIN contact c ON (myuser.user_id = c.user_id)
LEFT JOIN contact manager ON (myuser.manager_id = manager.user_id),
role r
WHERE myuser.role_id = r.role_id
AND myuser.user_id > 0
ORDER BY r.role,
c.namelast, c.namefirst,
myuser.username]]></queryString>
