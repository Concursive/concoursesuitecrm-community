	<queryString><![CDATA[SELECT t.ticketid, t.entered,
t.problem,  
t.solution,
t.resolution_date,
co.namefirst,
co.namelast,
o.org_id, o.name
FROM ticket t
LEFT JOIN organization o ON (t.org_id = o.org_id)
LEFT JOIN contact co ON (t.assigned_to = co.user_id)
WHERE t.ticketid > 0
AND t.closed IS NOT NULL
AND t.closed >= $P{date_start}
AND t.closed <= $P{date_end}
AND t.trashed_date IS NULL
AND t.ticketid NOT IN (SELECT ticket_id FROM ticketlink_project)
AND o.enabled = $P{TRUE}
$P!{orgid_where}
$P!{siteid_where}
ORDER BY o.name, t.entered]]></queryString>
