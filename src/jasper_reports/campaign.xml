  <parameter name="CENTRIC_DICTIONARY" isForPrompting="false" class="java.util.Map"/>
	<queryString><![CDATA[SELECT 
camp.campaign_id,
camp.name as campaign_name, 
camp.description as campaign_desc, 
camp.active_date, 
camp.entered,
ldo.description as send_type,
message.name as message_name,
message.subject,
owner.namelast as owner_last_name, 
owner.namefirst as owner_first_name,
survey.name as survey_name,
lst.description as survey_desc
FROM campaign camp
LEFT JOIN contact owner ON (camp.enteredby = owner.user_id) 
LEFT JOIN active_survey survey ON (camp.campaign_id = survey.campaign_id)
LEFT JOIN lookup_survey_types lst ON (survey.type = lst.code)
LEFT JOIN lookup_delivery_options ldo ON (camp.send_method_id = ldo.code)
LEFT JOIN message ON (camp.message_id = message.id)
WHERE 
camp.enteredby IN ($P!{userid_range})
AND camp.entered >= $P{date_range_start}
AND camp.entered <= $P{date_range_end}
ORDER BY camp.entered,
campaign_name,
owner_last_name, owner_first_name,
survey_name]]></queryString>
