	<queryString><![CDATA[SELECT o.name as name,
o.account_number as account_number,
o.namelast as namelast,
o.namefirst as namefirst,
o.namemiddle as namemiddle,
o.org_id as org_id,
o.employees as employees,
o.owner as owner,
o.contract_end as contract_end,
ct_owner.namelast as o_namelast,
ct_owner.namefirst as o_namefirst,
o.status_id
FROM organization o
LEFT JOIN contact ct_owner ON (o.owner = ct_owner.user_id)
WHERE o.org_id > 0
AND (o.status_id IS NULL OR o.status_id = 7)
AND o.trashed_date IS NULL
AND o.owner = $P{userid}
AND o.enabled = $P{TRUE}]]></queryString>
