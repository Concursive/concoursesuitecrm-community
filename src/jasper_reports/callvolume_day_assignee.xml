	<queryString><![CDATA[SELECT t.entered,
t.assigned_to,
co.namefirst,
co.namelast
FROM ticket t
LEFT JOIN contact co ON (t.assigned_to = co.user_id)
LEFT JOIN organization o ON (t.org_id = o.org_id)
WHERE t.ticketid > 0
AND o.enabled = $P{TRUE}
$P!{siteid_where}
AND t.trashed_date IS NULL
AND t.ticketid NOT IN (SELECT ticket_id FROM ticketlink_project)
AND t.entered >= $P{date_start}
AND t.entered <= $P{date_end}
ORDER BY $P!{year_part}, $P!{month_part}, $P!{day_part}, co.namelast, co.namefirst, t.assigned_to]]></queryString>
