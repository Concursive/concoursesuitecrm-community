	<queryString><![CDATA[SELECT o.org_id, t.ticketid, t.entered,
tc.description as category,
tc1.description as category1,
tc2.description as category2,
tc3.description as category3,
t.cat_code, t.subcat_code1, 
t.subcat_code2, t.subcat_code3
FROM ticket t
LEFT JOIN organization o ON (t.org_id = o.org_id)
LEFT JOIN ticket_category tc ON (t.cat_code = tc.id)
LEFT JOIN ticket_category tc1 ON (t.subcat_code1 = tc1.id)
LEFT JOIN ticket_category tc2 ON (t.subcat_code2 = tc2.id)
LEFT JOIN ticket_category tc3 ON (t.subcat_code3 = tc3.id)
WHERE t.ticketid > 0
$P!{siteid_where}
AND o.enabled = $P{TRUE}
AND t.trashed_date IS NULL
AND t.ticketid NOT IN (SELECT ticket_id FROM ticketlink_project)
AND t.entered >= $P{date_start}
AND t.entered <= $P{date_end}
ORDER BY $P!{year_part}, $P!{month_part},tc.description, tc1.description, tc2.description, tc3.description]]></queryString>
